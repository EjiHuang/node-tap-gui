<Application x:Class="NodeTapGui.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:Common="clr-namespace:Imagin.Common;assembly=Imagin.Common.WPF"
             xmlns:local="clr-namespace:NodeTapGui"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <Common:ThemeDictionary Assembly="Imagin.Common.WPF" Theme="Light" />
                <Common:StyleDictionary Assembly="Imagin.Common.WPF" />

                <!--<Common:StyleDictionary Assembly="NodeTapGui" />-->
                <ResourceDictionary Source="pack://application:,,,/NodeTapGui;component/Styles/Generic.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
